<?xml version="1.0" encoding="UTF-8"?>
<!-- This configuration file is used for both the Simulation Engine -->
<!-- and each Distributed System -->
<CabsfSimulationConfiguration xmlns="http://www.opensimulationsystems.org/cabsf/schemas/CabsfMessageExchange/0.1.0">
	<Name>JZombies CABSF JADE-Repast Simphony Simulation</Name>
	<!-- optional -->
	<ConfigVersion>1.0</ConfigVersion>
	<!-- optional -->
	<Author>
		<Name>Author name</Name>
		<Email>name@host.com</Email>
	</Author>
	<SimulationEngineSpecificConfigurations>
		<AllSimulationRuns>
			<SimulationEngineSpecific>
				<SimulationEngineName>REPAST_SIMPHONY</SimulationEngineName>
				<!-- These flags can be removed if/when the Repast team corrects these issues in their code. -->
				<!-- As of Repast Simphony 2.3.1, when using the CABSF Repast Simphony -->
				<!-- Simulation Runner (RSSR), the parameters are not read from the .rs -->
				<!-- directory. Apply temporary fix, to be removed in the future. -->
				<ApplyRssrParametersFix>true</ApplyRssrParametersFix>
				<!-- As of Repast Simphony 2.3.1, when adding objects to the context, Repast Simphony uses RandomHelper internally, which throws off the sequence of random numbers generated. The result is that the results of the simulation are different whether the RSSR is CABSF-enabled or not. This workaround corrects this issue, but does not correct the mismatch in results between the RSSR and regular GUI runs. -->
				<ApplyRssrRandomSeedContextAddFix>true</ApplyRssrRandomSeedContextAddFix>
			</SimulationEngineSpecific>
			<!-- optional, Default=Infinite -->
			<MaximumNumberOfTicks>300</MaximumNumberOfTicks>
			<!-- optional, Default=1 -->
			<NumberOfSimulationRuns>3</NumberOfSimulationRuns>
			<PseudorandomNumberSettings>
				<!-- Does not depend on the actual simulation engine used. -->
				<RandomNumberGenerator>REPAST_SIMPHONY</RandomNumberGenerator>
			</PseudorandomNumberSettings>
		</AllSimulationRuns>
		<!-- Simulation Run Configuration. Each child represents a run, sequentially. Optional -->
		<!-- Not Supported yet as of 6/27/2015 -->
		<IndividualSimulationRunConfigurations>
			<!-- optional, default is random number -->
			<SimulationRunConfiguration>
				<run>1</run>
				<seed>849365654</seed>
			</SimulationRunConfiguration>
			<SimulationRunConfiguration>
				<run>2</run>
				<seed>849365654</seed>
			</SimulationRunConfiguration>
			<!-- Example of default seed for third simulation run -->
			<SimulationRunConfiguration>
				<run>3</run>
			</SimulationRunConfiguration>
		</IndividualSimulationRunConfigurations>
		<ErrorHandling>
			<HaltSimulationRunOnTimeOut>true</HaltSimulationRunOnTimeOut>  <!-- Default=true -->
			<!-- Default=90 -->
			<TimeOutSeconds>120</TimeOutSeconds>
		</ErrorHandling>
	</SimulationEngineSpecificConfigurations>
	<AgentMappings>
		<DistributedSystems>
			<DistributedSystem>
				<!-- The Identifier for the MAS Identifier #1 of 3. -->
				<!-- The 3 identifiers jointly uniquely identify an ABM agent -->
				<DistributedSystemID>JADE_MAS1</DistributedSystemID>
				<!-- Map each JADE Agent with a JADE Agent Representation in Repast -->
				<DistributedAutonomousAgents>
					<!-- Attribute used internally by the simulation engine -->
					<!-- The distributed system(s) should not be concerned with this attribute value -->
					<AgentType type="jzombies.Human">
 		<!-- 				<DistributedAutonomousAgent>
							The JADE Agent ID. Identifier #2 of 3
							<DistributedAutonomousAgentID>DistributedSystemAutonomousAgent1</DistributedAutonomousAgentID>
							<AgentModels>
								The 1 to many agent models within the JADE agent
								Only one is supported currently
								<AgentModel>
									Identifier #3 of 3
									<DistributedAutonomousAgentModelID>DistributedSystemAutonomousAgent1MODEL</DistributedAutonomousAgentModelID>
									Only used internally by the simulation engine
									The distributed system(s) should not be concerned with this value
									<SimulationEngineClass>jzombies.Human</SimulationEngineClass>
								</AgentModel>
							</AgentModels>
						</DistributedAutonomousAgent> -->
						<DistributedAutonomousAgent>
							<!-- The JADE Agent ID. Identifier #2 of 3 -->
							<DistributedAutonomousAgentID>DistributedSystemAutonomousAgent2</DistributedAutonomousAgentID>
							<AgentModels>
								<!-- The 1 to many agent models within the JADE agent -->
								<!-- Only one is supported currently -->
								<AgentModel>
									<!-- Identifier #3 of 3 -->
									<DistributedAutonomousAgentModelID>DistributedSystemAutonomousAgent2MODEL</DistributedAutonomousAgentModelID>
									<!-- Only used internally by the simulation engine -->
									<!-- The distributed system(s) should not be concerned witt his value -->
									<SimulationEngineClass>jzombies.Human</SimulationEngineClass>
								</AgentModel>
							</AgentModels>
						</DistributedAutonomousAgent>
						<DistributedAutonomousAgent>
							<!-- The JADE Agent ID. Identifier #2 of 3 -->
							<DistributedAutonomousAgentID>DistributedSystemAutonomousAgent3</DistributedAutonomousAgentID>
							<AgentModels>
								<!-- The 1 to many agent models within the JADE agent -->
								<!-- Only one is supported currently -->
								<AgentModel>
									<!-- Identifier #3 of 3 -->
									<DistributedAutonomousAgentModelID>DistributedSystemAutonomousAgent3MODEL</DistributedAutonomousAgentModelID>
									<!-- Only used internally by the simulation engine -->
									<!-- The distributed system(s) should not be concerned witt his value -->
									<SimulationEngineClass>jzombies.Human</SimulationEngineClass>
								</AgentModel>
							</AgentModels>
						</DistributedAutonomousAgent>
						<DistributedAutonomousAgent>
							<!-- The JADE Agent ID. Identifier #2 of 3 -->
							<DistributedAutonomousAgentID>DistributedSystemAutonomousAgent4</DistributedAutonomousAgentID>
							<AgentModels>
								<!-- The 1 to many agent models within the JADE agent -->
								<!-- Only one is supported currently -->
								<AgentModel>
									<!-- Identifier #3 of 3 -->
									<DistributedAutonomousAgentModelID>DistributedSystemAutonomousAgent4MODEL</DistributedAutonomousAgentModelID>
									<!-- Only used internally by the simulation engine -->
									<!-- The distributed system(s) should not be concerned witt his value -->
									<SimulationEngineClass>jzombies.Human</SimulationEngineClass>
								</AgentModel>
							</AgentModels>
						</DistributedAutonomousAgent>
						<DistributedAutonomousAgent>
							<!-- The JADE Agent ID. Identifier #2 of 3 -->
							<DistributedAutonomousAgentID>DistributedSystemAutonomousAgent5</DistributedAutonomousAgentID>
							<AgentModels>
								<!-- The 1 to many agent models within the JADE agent -->
								<!-- Only one is supported currently -->
								<AgentModel>
									<!-- Identifier #3 of 3 -->
									<DistributedAutonomousAgentModelID>DistributedSystemAutonomousAgent5MODEL</DistributedAutonomousAgentModelID>
									<!-- Only used internally by the simulation engine -->
									<!-- The distributed system(s) should not be concerned witt his value -->
									<SimulationEngineClass>jzombies.Human</SimulationEngineClass>
								</AgentModel>
							</AgentModels>
						</DistributedAutonomousAgent>
						<DistributedAutonomousAgent>
							<!-- The JADE Agent ID. Identifier #2 of 3 -->
							<DistributedAutonomousAgentID>DistributedSystemAutonomousAgent6</DistributedAutonomousAgentID>
							<AgentModels>
								<!-- The 1 to many agent models within the JADE agent -->
								<!-- Only one is supported currently -->
								<AgentModel>
									<!-- Identifier #3 of 3 -->
									<DistributedAutonomousAgentModelID>DistributedSystemAutonomousAgent6MODEL</DistributedAutonomousAgentModelID>
									<!-- Only used internally by the simulation engine -->
									<!-- The distributed system(s) should not be concerned witt his value -->
									<SimulationEngineClass>jzombies.Human</SimulationEngineClass>
								</AgentModel>
							</AgentModels>
						</DistributedAutonomousAgent>
					</AgentType>
				</DistributedAutonomousAgents>
			</DistributedSystem>
		</DistributedSystems>
	</AgentMappings>
</CabsfSimulationConfiguration>
